    <%= link_to edit_account_team_path(@team), class: 'button-secondary button-smaller' do %>
      <%= t('teams.buttons.edit') %>
    <% end if can? :edit, @team %>
  <% end %>
<% end %>

<%= render 'account/shared/notices' %>

